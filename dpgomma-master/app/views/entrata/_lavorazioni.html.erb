<table class="table table-striped">
  <thead>
    <tr>
      <th>Pezzo</th>
      <th>Lavorazione</th>
      <th>Quantita</th>
      <th>Colli</th>
      <th>Peso</th>
      <th>Prezzo</th>
      <th>Iva</th>
      <th>TOTALE</th>

    </tr>
  </thead>

  <tbody>
    <% @entratum.entratadata.each do |entratadatum| %>
      <tr>
        <td><%= entratadatum.pezzo.nome %></td>
        <td><%= entratadatum.lavorazione.titolo %></td>
        <td><%= entratadatum.quantita %></td>
        <td><%= entratadatum.colli %></td>
        <td><%= entratadatum.peso %></td>
        <td><%= entratadatum.entratadatumtotale %></td>
        <td><%= entratadatum.entratumtotaleiva %></td>
        <td><%= entratadatum.entratumtotaleivato %></td>
      </tr>
    <% end %>
  </tbody>
</table>
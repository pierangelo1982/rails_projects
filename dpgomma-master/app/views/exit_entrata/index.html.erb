<h1>Listing exit_entrata</h1>

<table>
  <thead>
    <tr>
      <th>Exit</th>
      <th>Entratum</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exit_entrata.each do |exit_entratum| %>
      <tr>
        <td><%= exit_entratum.exit %></td>
        <td><%= exit_entratum.entratum %></td>
        <td><%= link_to 'Show', exit_entratum %></td>
        <td><%= link_to 'Edit', edit_exit_entratum_path(exit_entratum) %></td>
        <td><%= link_to 'Destroy', exit_entratum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exit entratum', new_exit_entratum_path %>
